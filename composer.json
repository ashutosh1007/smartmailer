{
  "name": "ashutosh1007/smartmailer",
  "description": "Smart Mailer for Laravel to handle dynamic from-address, SMTP rotation, failover, and monitoring.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/database": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/mail": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/routing": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/http": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/view": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "orchestra/testbench": "^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "SmartMailer\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ashutosh",
      "email": "lbi.ashutosh@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "SmartMailer\\SmartMailerServiceProvider"
      ],
      "aliases": {
        "SmartMailer": "SmartMailer\\Facades\\SmartMailer"
      }
    }
  }
}
